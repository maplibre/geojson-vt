{
  "name": "@maplibre/geojson-vt",
  "version": "5.0.1",
  "description": "Slice GeoJSON data into vector tiles efficiently",
  "type": "module",
  "exports": "./src/index.js",
  "sideEffects": false,
  "keywords": [
    "spatial",
    "geojson",
    "tiles",
    "geometry"
  ],
  "author": "Vladimir Agafonkin",
  "contributors": [
    "Ryan Sckully",
    "Harel Mazor"
  ],
  "module": "dist/geojson-vt.mjs",
  "main": "dist/geojson-vt-dev.js",
  "typings": "dist/index.d.ts",
  "jsdelivr": "dist/geojson-vt.js",
  "unpkg": "dist/geojson-vt.js",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/geojson": "^7946.0.16",
    "benchmark": "^2.1.4",
    "eslint": "^9.39.2",
    "eslint-config-mourner": "^4.1.0",
    "rollup": "^4.55.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "license": "ISC",
  "scripts": {
    "pretest": "eslint test/*.js debug/viz.js",
    "test": "node --test",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "bench": "node bench/benchmark.js",
    "docs": "mkdir -p docs && cp -r debug/* docs/ && find docs -name '*.html' -exec sed -i 's|../dist/geojson-vt-dev.js|https://unpkg.com/@maplibre/geojson-vt@latest/dist/geojson-vt.js|g' {} +",
    "prepublishOnly": "npm run test && npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maplibre/geojson-vt"
  }
}
